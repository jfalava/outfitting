{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Folder }} {{if .Root}}‚ùñ as Admin{{end}}",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#013e61",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\uf105",
          "type": "text"
        },
        {
          "foreground": "#d1c03b",
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#d1833b",
          "style": "plain",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#3a9c1f",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": "<#013e61>via</> \ue781 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "foreground": "#C94A16",
          "style": "plain",
          "template": "\uea87\uec03{{ reason .Code }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#f5f5a6",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": "\u27e8{{ .CurrentDate | date .Format }}\u27e9 <#eaeaea></>",
          "type": "time"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#007ACC",
          "style": "plain",
          "template": "{{ .Icon }}",
          "type": "os"
        },
        {
          "foreground": "#007ACC",
          "style": "plain",
          "template": "\uec03",
          "type": "text"
        },
        {
          "foreground": "#007ACC",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
