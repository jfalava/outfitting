{
  "show_edit_predictions": false,
  "notification_panel": {
    "dock": "left",
  },
  "features": {
    "edit_prediction_provider": "zed",
  },
  "auto_install_extensions": {
    "astro": true,
    "awk": true,
    "deno": true,
    "dockerfile": true,
    "docker-compose": true,
    "emmet": true,
    "env": true,
    "git-firefly": true,
    "html": true,
    "nginx": true,
    "ini": true,
    "php": true,
    "powershell": true,
    "pylsp": true,
    "rainbow-csv": true,
    "sql": true,
    "symbols": true,
    "terraform": true,
    "tmux": true,
    "tokyo-night": true,
    "toml": true,
    "unocss": true,
    "vscode-icons": true,
    "xcode-themes": true,
    "xml": true,
  },
  "icon_theme": "VSCode Icons (Dark)",
  "theme": {
    "mode": "system",
    "light": "Tokyo Night Light",
    "dark": "Xcode Classic Darker",
  },
  "base_keymap": "VSCode",
  "confirm_quit": true,
  "assistant": {
    "dock": "left",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest",
    },
    "version": "2",
  },
  "project_panel": {
    "dock": "right",
  },
  "soft_wrap": "editor_width",
  "ui_font_size": 18,
  "ui_font_family": "Pretendard Variable",
  "buffer_font_features": {
    "calt": true,
  },
  "buffer_font_size": 15.0,
  "buffer_font_weight": 400,
  "buffer_font_family": "Geist Mono",
  "terminal": {
    "dock": "right",
    "font_family": "GeistMono Nerd Font Propo",
    "font_size": 14,
    "font_weight": 400,
    // "shell": {
    //   "with_arguments": { "program": "pwsh", "args": [] },
    // },
    // uncomment if windows
  },
  "jupyter": {
    "kernel_selections": {
      "typescript": "deno",
      "javascript": "deno",
    },
  },
