{
  "show_edit_predictions": false,
  "notification_panel": {
    "dock": "left",
  },
  "features": {
    "edit_prediction_provider": "zed",
  },
  "auto_install_extensions": {
    "astro": true,
    "awk": true,
    "deno": true,
    "dockerfile": true,
    "docker-compose": true,
    "emmet": true,
    "env": true,
    "git-firefly": true,
    "html": true,
    "nginx": true,
    "ini": true,
    "php": true,
    "powershell": true,
    "pylsp": true,
    "rainbow-csv": true,
    "sql": true,
    "symbols": true,
    "terraform": true,
    "tmux": true,
    "tokyo-night": true,
    "toml": true,
    "unocss": true,
    "vscode-icons": true,
    "xcode-themes": true,
    "xml": true,
    "zed-legacy-themes": true,
  },
  "icon_theme": {
    "mode": "system",
    "light": "VSCode Icons (Light)",
    "dark": "VSCode Icons (Dark)",
  },
  "theme": {
    "mode": "system",
    "light": "Xcode Bare",
    "dark": "Zed Legacy:  Andromeda",
  },
  "base_keymap": "VSCode",
  "confirm_quit": true,
  "assistant": {
    "dock": "left",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest",
    },
    "version": "2",
  },
  "project_panel": {
    "dock": "right",
  },
  "ui_font_size": 14,
  "ui_font_family": "Pretendard Variable",
  "buffer_font_features": {
    "calt": true,
  },
  "buffer_font_size": 15.0,
  "buffer_font_weight": 400,
  "buffer_font_family": "Geist Mono",
  "soft_wrap": "editor_width",
  "show_wrap_guides": true,
  "tabs": {
    "close_position": "right",
    "file_icons": true,
    "git_status": true,
    "activate_on_close": "history",
    "show_close_button": "hover",
  },
  "terminal": {
    "dock": "left",
    "font_family": "GeistMono Nerd Font Propo",
    "font_size": 14,
    "font_weight": 400,
  },
  "shell": {
    "with_arguments": { "program": "pwsh" },
  },
  "jupyter": {
    "kernel_selections": {
      "typescript": "deno",
      "javascript": "deno",
    },
  },
  "telemetry": {
    "diagnostics": true,
    "metrics": true,
  },
  "lsp": {
