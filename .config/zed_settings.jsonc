{
  "icon_theme": "Bearded Icon Theme",
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null,
      "enterprise_uri": null,
    },
    "enabled_in_text_threads": false,
  },
  "features": {
    "edit_prediction_provider": "zed",
  },
  "collaboration_panel": {
    "button": false,
    "dock": "left",
    "default_width": 240,
  },
  "soft_wrap": "editor_width",
  "preferred_line_length": 80,
  "show_wrap_guides": true,
  "git_panel": {
    "dock": "right",
  },
  "agent": {
    "default_model": {
      "provider": "openrouter",
      "model": "x-ai/grok-4-fast:free",
    },
    "single_file_review": false,
    "dock": "left",
  },
  "project_panel": {
    "dock": "right",
  },
  "ui_font_size": 18,
  "ui_font_family": "Pretendard Variable",
  "tab_size": 2,
  "buffer_font_size": 16.5,
  "buffer_font_family": "Google Sans Code",
  "buffer_font_weight": 500,
  "terminal": {
    "font_family": "GeistMono Nerd Font Propo",
    "font_size": 15,
    "font_weight": 300,
    "shell": {
      "program": "pwsh.exe",
    },
  },
  "theme": {
    "mode": "system",
    "light": "Github Light",
    "dark": "poimandres blurry",
  },
}
