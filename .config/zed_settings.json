      "binary": {
        "path": "C:\\Program Files\\nodejs\\node.exe",
        "arguments": [
          "C:\\Users\\jalava\\AppData\\Local\\Zed\\extensions\\work\\emmet\\node_modules\\@olrtg\\emmet-language-server\\dist\\index.js",
          "--stdio"
        ]
      }
    },
    "astro-language-server": {
      "binary": {
        "path": "C:\\Program Files\\nodejs\\node.exe",
        "arguments": [
          "C:\\Users\\jalava\\AppData\\Local\\Zed\\extensions\\work\\emmet\\node_modules\\@astrojs\\language-server\\dist\\index.js",
          "--stdio"
        ]
      }
    }
  },
  "jupyter": {
