  "tab_size": 2,
  "buffer_font_size": 19.0,
  "buffer_font_family": "Google Sans Code",
  "buffer_font_weight": 500,
  "terminal": {
    "dock": "left",
    "font_family": "VictorMono Nerd Font Propo",
    "font_size": 17.0,
    "font_weight": 800,
  },
  "theme": {
    "mode": "system",
    "light": "Github Light",
    "dark": "Bearded Theme Arc",
  },
